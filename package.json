{
  "name": "greetings",
  "displayName": "greetings",
  "description": "VSCode extension for greetings",
  "version": "0.0.1",
  "publisher": "AkashGutha",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.greet"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "extension.greet",
        "title": "Greet"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "unirest": "^0.5.1",
    "vscode": "^1.0.0"
  }
}
